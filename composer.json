{
    "name": "smsgate/client",
    "description": "PHP Client for using SMS Gate HTTP API.",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "php-http/client-implementation": "^1.0",
        "zendframework/zend-diactoros": "^1.3",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "SMSGate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SMSGate\\": "test/"
        }
    }
}